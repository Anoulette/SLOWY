<%= simple_form_for(@vehicle) do |f| %>
  <%= f.input :name %>
  <%= f.input :vehicle_type %>
  <%= f.input :details %>
  <%= f.input :price %>
  <%= f.input :street_number %>
  <%= f.input :street %>
  <%= f.input :zipcode %>
  <%= f.input :city %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
