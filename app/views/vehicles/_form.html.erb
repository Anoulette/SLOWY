<%= simple_form_for(@vehicle) do |f| %>
  <%= f.input :name %>
  <%= f.select :vehicle_type, [['Bike','bike'],['E-bike','e-bike'],['Cargo bike','cargo bike'],['Overboard','overboard'],['Scooter','scooter'],['E-scooter','e-scooter'],['E-unicycle','e-unicycle']] %>
  <%= f.input :details %>
  <%= f.input :price %>
  <%= f.input :street_number %>
  <%= f.input :street %>
  <%= f.input :zipcode %>
  <%= f.input :city %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
